                    "borderRadius": 20,
                    "child": {
                      "flutterType": "InkWell",
                      "customBorder": {
                        "flutterType": "RoundedRectangleBorder",
                        "borderRadius": 20
                      },
                      "onTap": ":openWindow(onTapEditAccount)",
                      "splashColor": "grey.200",
                      "highlightColor": "white",
