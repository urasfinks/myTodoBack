              "margin": "30,0,30,0",
              "child": {
                "flutterType": "SizedBox",
                "width": "infinity",
                "height": 43,
                "child": {
                  "flutterType": "ElevatedButtonIcon",
                  "icon": {
                    "flutterType": "Icon",
                    "src": "telegram",
                    "size": 18,
                    "color": "white"
