{
  "flutterType": "Container",
  "width": "infinity",
  "decoration": {
    "flutterType": "BoxDecoration",
    "gradient": {
      "flutterType": "LinearGradient",
      "begin": "topRight",
      "colors": [
        "blue.600",
        "blue.700",
        "blue.800"
      ]
    }
  },
  "child": {
    "flutterType": "Column",
    "crossAxisAlignment": "center",
    "children": [
      {
        "flutterType": "Container",
        "padding": "0,0,30,0",
        "alignment": "topRight",
        "child": {
          "flutterType": "InkWell",
          "onTap": ":openWindow(onTapChat)",
          "splashColor": "grey.200",
          "highlightColor": "white",
          "child": {
            "flutterType": "Row",
            "mainAxisAlignment": "end",
            "children": [
              {
                "flutterType": "Icon",
                "src": "notifications_active",
                "color": "#ffffff"
              },
              {
                "flutterType": "Text",
                "data": " ${count_chat_unread}",
                "style": {
                  "flutterType": "TextStyle",
                  "fontSize": 17,
                  "color": "#ffffff"
                }
              }
            ]
          }
        }
      },
      {
        "flutterType": "InkWell",
        "onTap": ":openGallery(onTapAvatarSet)",
        "child": {
          "flutterType": "CircleAvatar",
          "radius": 82,
          "backgroundColor": "rgba:255,255,255,0.1",
          "child": {
            "flutterType": "CircleAvatar",
            "radius": 70,
            "backgroundImage": {
              "flutterType": "CachedNetworkImageProvider",
              "src": "http://jamsys.ru:8081/avatar-get?time=${time}"
            }
          }
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 10
      },
      {
        "flutterType": "Text",
        "data": "${fio}",
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 24,
          "color": "#ffffff"
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 10
      },
      {
        "flutterType": "Text",
        "data": "${bday}",
        "style": {
          "flutterType": "TextStyle",
          "fontSize": 14,
          "color": "rgba:255,255,255,0.5"
        }
      },
      {
        "flutterType": "SizedBox",
        "height": 20
      },
      {
        "flutterType": "Container",
        "decoration": {
          "flutterType": "BoxDecoration",
          "color": "#f5f5f5",
          "borderRadius": "20,20,0,0"
