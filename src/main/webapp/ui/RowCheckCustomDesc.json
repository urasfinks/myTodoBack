{
  "flutterType": "Container",
  "padding": "10,6,8,6",
  "alignment": "left",
  "color": "${color}",
  "child": {
    "flutterType": "Row",
    "crossAxisAlignment": "center",
    "children": [
      {
        "flutterType": "Container",
        "margin": "0,0,10,0",
        "height": 35,
        "width": 5,
        "decoration": {
          "flutterType": "BoxDecoration",
          "color": "${tagColor}",
          "borderRadius": 20
        }
      },
      {
        "flutterType": "Expanded",
        "child": {
          "flutterType": "Column",
          "crossAxisAlignment": "start",
          "children": [
            {
              "flutterType": "Text",
              "data": "${title}",
              "style": {
                "flutterType": "TextStyle",
                "fontSize": 16,
                "color": "${titleColor}"
              }
            },
            {
              "flutterType": "SizedBox",
              "height": 3
            },
            {
              "flutterType": "Text",
              "data": "${desc}",
              "style": {
                "flutterType": "TextStyle",
                "color": "${descColor}",
                "fontSize": 12
              }
            }
          ]
        }
      },
      {
        "flutterType": "SizedBox",
        "width": 10
      },
      {
        "flutterType": "AppStore",
        "child": {
          "flutterType": "Checkbox",
          "borderColor": "${descColor}",
          "value": "=>getAppStore(getAppStoreDataChecked)",
          "name": "${nameChecked}"
        }
      },
      {
        "flutterType": "IconButton",
        "icon": {
          "flutterType": "Icon",
          "src": "${icon_edit}",
          "color": "${descColor}"
        },
        "onPressed": ":openWindow(onPressedData)"
      }
    ]
  }
}