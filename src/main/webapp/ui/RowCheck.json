{
  "flutterType": "Container",
  "padding": "15,6,8,6",
  "alignment": "left",
  "color": "${color}",
  "child": {
    "flutterType": "Row",
    "crossAxisAlignment": "center",
    "children": [
      {
        "flutterType": "Expanded",
        "child": {
          "flutterType": "Column",
          "crossAxisAlignment": "start",
          "children": [
            {
              "flutterType": "Text",
              "data": "${title}",
              "style": {
                "flutterType": "TextStyle",
                "fontSize": 16,
                "color": "${titleColor}"
              }
            },
            {
              "flutterType": "SizedBox",
              "height": 3
            },
            {
              "flutterType": "AppStore",
              "child": {
                "flutterType": "Text",
                "data": "=>getAppStore(getAppStoreDataTime)|timestampToDate(getAppStoreDataTime)",
                "style": {
                  "flutterType": "TextStyle",
                  "color": "${descColor}",
                  "fontSize": 12
                }
              }
            }
          ]
        }
      },
      {
        "flutterType": "SizedBox",
        "width": 10
      },
      {
        "flutterType": "AppStore",
        "child": {
          "flutterType": "Checkbox",
          "borderColor": "${descColor}",
          "value": "=>getAppStore(getAppStoreDataChecked)",
          "name": "${nameChecked}"
        }
      },
      {
        "flutterType": "IconButton",
        "icon": {
          "flutterType": "Icon",
          "src": "more_vert",
          "color": "${descColor}"
        },
        "onPressed": ":openWindow(onPressedData)"
      }
    ]
  }
}