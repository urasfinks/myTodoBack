{
  "flutterType": "Container",
  "margin": "10,20,${offsetRight},20",
  "child": {
    "flutterType": "Row",
    "crossAxisAlignment": "start",
    "children": [
      {
        "flutterType": "Expanded",
        "child": {
          "flutterType": "Text",
          "data": "${title}",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 16,
            "fontWeight": "bold",
            "color": "#999999"
          }
        }
      },
      {
        "flutterType": "Container",
        "alignment": "center",
        "padding": "7,2,7,2",
        "decoration": {
          "flutterType": "BoxDecoration",
          "borderRadius": "5",
          "color": "#ffffff"
        },
        "child": {
          "flutterType": "Text",
          "data": "${extra}",
          "style": {
            "flutterType": "TextStyle",
            "fontSize": 12
          }
        }
      }
    ]
  }
}