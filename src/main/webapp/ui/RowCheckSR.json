{
  "flutterType": "Material",
  "color": "${color}",
  "child": {
    "flutterType": "Slidable",
    "children": [
      {
        "flutterType": "SizedBox",
        "width": 100,
        "child": {
          "flutterType": "Row",
          "mainAxisAlignment": "center",
          "children": [
            {
              "flutterType": "RawMaterialButton",
              "fillColor": "red",
              "padding": 10,
              "onPressed": "${onPressedRemove}",
              "icon": {
                "flutterType": "Icon",
                "src": "delete_outline",
                "color": "white",
                "size": 20
              }
            }
          ]
        }
      }
    ],
    "child": {
      "flutterType": "InkWell",
      "onTap": ":openWindow(onPressedData)",
      "splashColor": "grey.200",
      "highlightColor": "white",
      "child": {
        "flutterType": "Container",
        "padding": "10,6,8,6",
        "alignment": "left",
        "child": {
          "flutterType": "Row",
          "crossAxisAlignment": "center",
          "children": [
            {
              "flutterType": "Container",
              "margin": "0,0,10,0",
              "height": 35,
              "width": 5,
              "decoration": {
                "flutterType": "BoxDecoration",
                "color": "${tagColor}",
                "borderRadius": 20
              }
            },
            {
              "flutterType": "Expanded",
              "child": {
                "flutterType": "Column",
                "crossAxisAlignment": "start",
                "children": [
                  {
                    "flutterType": "SizedBox",
                    "height": 4
                  },
                  {
                    "flutterType": "Text",
                    "data": "${title}",
                    "style": {
                      "flutterType": "TextStyle",
                      "fontSize": 16,
                      "color": "${titleColor}"
                    }
                  },
                  {
                    "flutterType": "SizedBox",
                    "height": 3
                  },
                  {
                    "flutterType": "Text",
                    "data": "=>getAppStore(getAppStoreDataTime)|timestampToDate(getAppStoreDataTime)",
                    "style": {
                      "flutterType": "TextStyle",
                      "color": "${descColor}",
                      "fontSize": 12
                    }
                  },
                  {
                    "flutterType": "SizedBox",
                    "height": 4
                  }
                ]
              }
            },
            {
              "flutterType": "Checkbox",
              "borderColor": "${descColor}",
              "value": "=>getAppStore(getAppStoreDataChecked)",
              "name": "${nameChecked}"
            },
            {
              "flutterType": "SizedBox",
              "width": 24,
              "child": {
                "flutterType": "Icon",
                "src": "${icon_edit}",
                "color": "${descColor}",
                "size": "${icon_size}"
              }
            }
          ]
        }
      }
    }
  }
}

